@{
    ViewData["Title"] = "Home Page";
}

@model Dictionary<string, long>

<table border="1">
    <thead>
        <tr>
            <th>Name</th>
            <th>Total Time Worked</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr style="@(employee.Value < 100 ? "background-color: #e96e6e;" : "")">
                <td>@employee.Key</td>
                <td>@employee.Value</td>
            </tr>
        }
    </tbody>
</table>
 